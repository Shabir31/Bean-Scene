@model RestaurantTable
@{
    ViewData["Title"] = "DeleteRestaurantTable";
}

<h1>Delete A Restaurant Table</h1>

<hr />

<form asp-controller="RestaurantTable" asp-action="DeleteRestaurantTable" method="post" autocomplete="off" class="p-5">

    <div class="mb-3">
        <label class="form-label"> Id</label>
        <input type="text" class="form-control" asp-for="Id" readonly />
    </div>

    <div class="mb-3">
        <label class="form-label">Restaurant Table Name</label>
        <input type="text" class="form-control" asp-for="SeatName" readonly />
    </div>

     <div class="mb-3">
        <label class="form-label"> Dining Area Name </label>
        <input type="text" class="form-control" asp-for="DiningAreaId" readonly />
    </div>

    <div class="d-grid">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>

</form>