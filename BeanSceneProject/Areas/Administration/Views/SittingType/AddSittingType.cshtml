@model SittingType
@{
    ViewData["Title"] = "AddSittingType";
}

<h1>Add A Sitting Type</h1>

<hr />

<form asp-controller="SittingType" asp-action="AddSittingType" method="post" autocomplete="off" class="p-5">
    <div class="mb-3">
        <label class="form-label">Sitting Type</label>
        <input type="text" class="form-control" asp-for="SittingTypeName" />
        <span asp-validation-for="SittingTypeName" class="text-danger" />
    </div>
    <div class="d-grid">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>