@model SittingType
@{
    ViewData["Title"] = "DeleteSittingType";
}

<h1>Delete A Sitting Type</h1>

<hr />

<form asp-controller="SittingType" asp-action="DeleteSittingType" method="post" autocomplete="off" class="p-5">

    <div class="mb-3">
        <label class="form-label"> Id</label>
        <input type="text" class="form-control" asp-for="Id" readonly />
    </div>

    <div class="mb-3">
        <label class="form-label">Sitting Type Name</label>
        <input type="text" class="form-control" asp-for="SittingTypeName" readonly />
    </div>

    <div class="d-grid">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>

</form>